" vim -u ~/.vim/vimrc.min
if has('vim_starting')
  set nocompatible
endif

set undofile
set packpath=
" set runtimepath+=~/.config/nvim/pack/minpac/opt/lightline.vim
set runtimepath+=~/.vim/pack/minpac/start/iceberg.vim
set runtimepath+=~/.vim/pack/minpac/start/vim-airline

if !has('gui_running')
  set t_Co=256
endif

filetype plugin indent on
syntax on

colorscheme iceberg
set background=light
